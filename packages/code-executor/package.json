{
  "name": "@lisa/code-executor",
  "version": "1.0.0",
  "description": "Python code execution engine using Pyodide - Run Python in the browser",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./engine": {
      "import": "./dist/engine.mjs",
      "require": "./dist/engine.js",
      "types": "./dist/engine.d.ts"
    },
    "./react": {
      "import": "./dist/react.mjs",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    }
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "python",
    "pyodide",
    "code-execution",
    "jupyter",
    "notebook",
    "wasm",
    "webassembly",
    "lisa-ai"
  ],
  "author": "Lisa AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lisa-ai/code-executor"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "@lisa-sdk/core": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
